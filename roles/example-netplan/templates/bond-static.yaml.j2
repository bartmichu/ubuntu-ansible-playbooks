## Use netplan-try to test new configuration

network:
  version: 2
  renderer: networkd
  ethernets:
    eports:
      match:
        name: xxx*
      optional: true
  bonds:
    bond0:
      interfaces: [eports]
      dhcp4: false
      addresses: [{{ ip_address }}/24]
      gateway4: {{ ip_gateway }}
      nameservers:
        addresses: [{{ dns_servers }}]
      parameters:
        mode: balance-tlb
